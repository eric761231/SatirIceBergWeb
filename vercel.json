{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "source": "/test-pwa", "destination": "/test-pwa.html" },
    { "source": "/pwa-config.js", "destination": "/pwa-config.js" },
    { "source": "/service-worker.js", "destination": "/service-worker.js" },
    { "source": "/sw.js", "destination": "/sw.js" },
    { "source": "/manifest.json", "destination": "/manifest.json" },
    { "source": "/icons/(.*)", "destination": "/icons/$1" },
    { "source": "/src/(.*)", "destination": "/src/$1" },
    { "source": "/:file.js", "destination": "/:file.js" },
    { "source": "/:file.css", "destination": "/:file.css" },
    { "source": "/:file.png", "destination": "/:file.png" },
    { "source": "/:file.jpg", "destination": "/:file.jpg" },
    { "source": "/:file.jpeg", "destination": "/:file.jpeg" },
    { "source": "/:file.gif", "destination": "/:file.gif" },
    { "source": "/:file.svg", "destination": "/:file.svg" },
    { "source": "/:file.woff", "destination": "/:file.woff" },
    { "source": "/:file.woff2", "destination": "/:file.woff2" },
    { "source": "/:file.ttf", "destination": "/:file.ttf" },
    { "source": "/:file.eot", "destination": "/:file.eot" },
    { "source": "/:file.ico", "destination": "/:file.ico" },
    { "source": "/", "destination": "/index.html" },
    { "source": "/meditation", "destination": "/meditation.html" },
    { "source": "/app", "destination": "/meditation.html" },
    { "source": "/mandala.html", "destination": "/mandala.html" },
    { "source": "/articles.html", "destination": "/articles.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/service-worker.js",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }]
    },
    {
      "source": "/sw.js",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }]
    },
    {
      "source": "/manifest.json",
      "headers": [{ "key": "Content-Type", "value": "application/manifest+json" }]
    },
    {
      "source": "/pwa-config.js",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=3600" }]
    }
  ]
}
